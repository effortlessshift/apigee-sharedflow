<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout async="false" continueOnError="false" enabled="true" name="SC-OktaFactorsList">
    <DisplayName>SC-OktaFactorsList</DisplayName>
    <Request clearPayload="true" variable="getFactorsListRequest">
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
        <Set>
            <Headers>
                <Header name="Authorization">SSWS {private.KVMoktAPIKey}</Header>
            </Headers>
            <Verb>GET</Verb>
        </Set>
    </Request>
    <Response>getFactorsListResponse</Response>
    <Timeout>60000</Timeout>
    <HTTPTargetConnection>
        <Properties>
            <Property name="success.codes">2xx,4xx,5xx</Property>
            <Property name="keepalive.timeout.millis">0</Property>
            <Property name="connect.timeout.millis">7000</Property>
        </Properties>
        <LoadBalancer>
            <Server name="oktabackend"/>
        </LoadBalancer>
        <Path>api/v1/users/{extractedSsoId}/factors</Path>
    </HTTPTargetConnection>
</ServiceCallout>